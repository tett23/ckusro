{
  "presets": [
    ["@babel/preset-typescript", {
      "isTSX": true,
      "allExtensions": true
    }],
    ["@babel/preset-env", {
      "modules": "commonjs",
      "targets": {
        "node": "current"
      },
      "useBuiltIns": "entry"
    }],
    "@babel/preset-react",
  ],
  "plugins": [
    "syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
