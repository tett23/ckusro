{
  "name": "@ckusro/ckusro-web",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "tett23 <tett23@gmail.com>",
  "homepage": "https://github.com/tett23/ckusro/tree/master/packages/ckusro-web",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tett23/ckusro.git"
  },
  "scripts": {
    "start": "rm -rf lib/ && parcel serve src/index.html -d lib --no-autoinstall --port 3001",
    "build": "rm -rf lib/ && parcel serve src/index.html -d lib --no-autoinstall --port 3001",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "bugs": {
    "url": "https://github.com/tett23/ckusro/issues"
  },
  "dependencies": {
    "@ckusro/ckusro-core": "0.0.0",
    "@isomorphic-git/lightning-fs": "^3.2.1",
    "core-js": "^3.1.2",
    "react": "16.8.3",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.8.6",
    "react-native": "^0.59.8",
    "react-native-web": "^0.11.2",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/node": "^12.0.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.8.0",
    "memfs": "^2.15.4",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-react-native-web": "^0.6.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "unionfs": "^4.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "setupFiles": [
      "./__tests__/setup.ts"
    ],
    "testRegex": "(models/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ],
    "testURL": "http://localhost/"
  },
  "alias": {
    "react-native": "react-native-web"
  }
}
