{
  "name": "@ckusro/ckusro-web",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "tett23 <tett23@gmail.com>",
  "homepage": "https://github.com/tett23/ckusro/tree/master/packages/ckusro-web",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tett23/ckusro.git"
  },
  "scripts": {
    "start": "rm -rf lib/ && parcel serve src/index.html -d lib --no-autoinstall --port 3001",
    "build": "rm -rf lib/ && parcel build src/index.html -d lib",
    "build-watch": "rm -rf lib/ && parcel watch src/index.html -d lib",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "bugs": {
    "url": "https://github.com/tett23/ckusro/issues"
  },
  "dependencies": {
    "@ckusro/ckusro-core": "0.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@isomorphic-git/lightning-fs": "^3.2.1",
    "core-js": "^3.1.2",
    "react": "16.8.3",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.8.6",
    "react-native": "^0.59.8",
    "react-native-web": "^0.11.2",
    "react-primitives": "^0.8.0",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@types/node": "^12.0.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "memfs": "^2.15.4",
    "parcel-bundler": "^1.12.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "unionfs": "^4.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    },
    "setupFiles": [
      "./__tests__/setup.ts"
    ],
    "testRegex": "(models/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ],
    "testURL": "http://localhost/"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "8.10",
            "browsers": "> 0.25%, not dead"
          },
          "spec": true,
          "loose": true,
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "^react-native$": "react-native-web"
          }
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-object-rest-spread",
        {},
        "foo"
      ],
      "babel-plugin-styled-components"
    ]
  }
}
